CLANG_ALLOW_NON_MODULAR_INCLUDES_IN_FRAMEWORK_MODULES = YES
GCC_PREPROCESSOR_DEFINITIONS = $(inherited) COCOAPODS=1
HEADER_SEARCH_PATHS = $(SDKROOT)/usr/include/libxml2 "$(SDKROOT)/usr/include/libxml2" $(inherited) "${PODS_ROOT}/Headers/Public" "${PODS_ROOT}/Headers/Public/ImageFilters" "${PODS_ROOT}/Headers/Public/KissXML" "${PODS_ROOT}/Headers/Public/SS-KFEpubKit" "${PODS_ROOT}/Headers/Public/SSZipArchive"
OTHER_CFLAGS = $(inherited) -DDDXML_LIBXML_MODULE_ENABLED=0 $(inherited) -isystem "${PODS_ROOT}/Headers/Public" -isystem "${PODS_ROOT}/Headers/Public/ImageFilters" -isystem "${PODS_ROOT}/Headers/Public/KissXML" -isystem "${PODS_ROOT}/Headers/Public/SS-KFEpubKit" -isystem "${PODS_ROOT}/Headers/Public/SSZipArchive"
OTHER_LDFLAGS = $(inherited) -ObjC -l"ImageFilters" -l"KissXML" -l"SS-KFEpubKit" -l"SSZipArchive" -l"xml2" -l"z" -framework "Accelerate" -framework "CoreImage" -framework "Foundation" -framework "UIKit"
PODS_ROOT = ${SRCROOT}/Pods